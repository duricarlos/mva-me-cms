{
  "kind": "collectionType",
  "collectionName": "sales_points",
  "info": {
    "singularName": "sales-point",
    "pluralName": "sales-points",
    "displayName": "sales-point"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "coordinates": {
      "type": "json",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "distribuidor",
        "ferreteria",
        "deposito"
      ]
    },
    "description": {
      "type": "string"
    },
    "seller_profiles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::seller-profile.seller-profile",
      "inversedBy": "sales_points"
    }
  }
}
